    return jsh.App.xform_<%=model.id%>.State;
  }

  jsh.App.XForm<%=model.id%>_LoadState = function(state){
    if('Sort' in state){
      jsh.App.xform_<%=model.id%>.State.Sort = state.Sort;
      jsh.App.xform_<%=model.id%>.Sort = state.Sort;
      jsh.App.xform_<%=model.id%>.ResetSortGlyphs(jsh.$root('.xform<%=model.id%>'));
    }
    if('Search' in state){
      jsh.App.xform_<%=model.id%>.State.Search = state.Search;
      jsh.App.xfilter_<%=model.id%>.Data.Items = state.Search;
      jsh.App.xfilter_<%=model.id%>.Render();
      jsh.App.xform_<%=model.id%>.SearchJSON = JSON.stringify(state.Search);
    }
  }

  jsh.App.XForm<%=model.id%>_Refresh = function(){
    if(jsh.XForm_OnGridExit(null,function(){ jsh.App.XForm<%=model.id%>_Refresh(); })) return;
    jsh.App.xform_<%=model.id%>.Load();
  }

  jsh.App.XForm<%=model.id%>_Sort = function(obj,fieldname){
    if(!jsh.App.xform_<%=model.id%>.Prop.isInlinePopup) if(jsh.XForm_OnGridExit(obj,function(){ jsh.App.XForm<%=model.id%>_Sort(obj,fieldname); })) return;
    jsh.App.xform_<%=model.id%>.AddSort(obj,fieldname);
    jsh.App.xform_<%=model.id%>.State.Sort = jsh.App.xform_<%=model.id%>.Sort;
    jsh.XForm_AddHistory();
    return false;
  }

  jsh.App.XForm<%=model.id%>_HasSearchItems = function(val){
    var rslt = false;
    if(typeof val == 'undefined') val = jsh.App.xform_<%=model.id%>.SearchJSON;
    if(val.trim() != ''){
      var sjson = JSON.parse(val);
      _.each(sjson,function(squery){
        if(squery.Value.trim() != '') rslt = true;
        if((squery.Comparison == 'null')||(squery.Comparison == 'notnull')) rslt = true; 
      });
    }
    return rslt;
  }

  jsh.App.XForm<%=model.id%>_FilterColumnChange = function(){
    jsh.App.xfilter_<%=model.id%>.GetValues();
    jsh.App.xfilter_<%=model.id%>.Render();
  }

  jsh.App.XForm<%=model.id%>_FilterComparisonChange = function(){
    jsh.App.xfilter_<%=model.id%>.GetValues();
    jsh.App.xfilter_<%=model.id%>.Render();
  }

  jsh.App.XForm<%=model.id%>_FilterJoinChange = function(){
    jsh.App.xfilter_<%=model.id%>.GetValues();
    var d = jsh.App.xfilter_<%=model.id%>.Data;
    if(d.Items.length > 0){
      var curjoin = d.Items[0].Join;
      if(curjoin == 'and') curjoin = 'or';
      else curjoin = 'and';
      for(var i=0;i<d.Items.length;i++){
        d.Items[i].Join = curjoin;
      }
      jsh.App.xfilter_<%=model.id%>.Render();
    }
  }

  jsh.App.XForm<%=model.id%>_FilterExpand = function(timeout){
    if(jsh.$root('.xfilter_<%=model.id%>').is(':visible')) return;
    if(jsh.App.xfilter_<%=model.id%>.Data.Items.length == 0){
      jsh.App.XForm<%=model.id%>_FilterAdd();
      jsh.$root('.xfilter_<%=model.id%>').slideDown(timeout,function(){ jsh.$root('.xfilter_<%=model.id%> .xfilter_value').first().select().focus(); });
    }
    else {
      jsh.$root('.xfilter_<%=model.id%>').slideDown(timeout,function(){ jsh.$root('.xfilter_<%=model.id%> .xfilter_value').first().select().focus(); });
    }
  }

  jsh.App.XForm<%=model.id%>_FilterButton = function(){
    if(jsh.$root('.xfilterbutton<%=model.id%> span').html()=='Remove Filter'){
      jsh.App.xfilter_<%=model.id%>.Data.Items = [];
      jsh.App.xfilter_<%=model.id%>.Render();
      jsh.$root('.xfilter_<%=model.id%>').hide();
      jsh.App.XForm<%=model.id%>_FilterExpand(400);
      jsh.App.XForm<%=model.id%>_FilterApply();
    }
    else{
      if(jsh.$root('.xfilter_<%=model.id%>').is(':visible')) jsh.$root('.xfilter_<%=model.id%>').slideUp();
      else jsh.App.XForm<%=model.id%>_FilterExpand(400);
    }
  }

  jsh.App.XForm<%=model.id%>_FilterKeyUp = function(event){
    if(event.keyCode==13) jsh.App.XForm<%=model.id%>_FilterApply();
  }

  jsh.App.XForm<%=model.id%>_SetFilter = function(searchitems,hide){
    if(hide) jsh.$root('.xfilter_<%=model.id%>').hide();
    jsh.App.xfilter_<%=model.id%>.Data.Items = searchitems;
    jsh.App.xfilter_<%=model.id%>.Render();
    jsh.App.xform_<%=model.id%>.State.Search = searchitems;
    jsh.App.xform_<%=model.id%>.SearchJSON = JSON.stringify(searchitems);
  }

  jsh.App.XForm<%=model.id%>_FilterApply = function(cb){
    if(!jsh.App.xform_<%=model.id%>.Prop.isInlinePopup) if(jsh.XForm_OnGridExit(null,function(){ jsh.App.XForm<%=model.id%>_FilterApply(); })) return;
    jsh.App.xfilter_<%=model.id%>.GetValues();
    var searchitems = JSON.parse(JSON.stringify(jsh.App.xfilter_<%=model.id%>.Data.Items));
    var has_search_items = false;
    _.each(searchitems,function(val){ 
      if(val.Value.trim() != '') has_search_items = true; 
      if((val.Comparison == 'null')||(val.Comparison == 'notnull')) has_search_items = true; 
    });
    //if (val.trim() != '') 
    var sjson = '';
    if(has_search_items){ 
      sjson = JSON.stringify(searchitems);
    }
    jsh.App.xform_<%=model.id%>.State.Search = searchitems;
    jsh.XForm_AddHistory();
    jsh.App.xform_<%=model.id%>.NewSearchJSON(sjson, cb);
  }

  jsh.App.XForm<%=model.id%>_FilterAdd = function(){
    jsh.App.xfilter_<%=model.id%>.GetValues();
    jsh.App.xfilter_<%=model.id%>.Data.Items.unshift(new jsh.XSearch.SearchItem('ALL','','and','contains'));
    jsh.App.xfilter_<%=model.id%>.Render();
  }

  jsh.App.XForm<%=model.id%>_FilterRemove = function(obj){
    jsh.App.xfilter_<%=model.id%>.GetValues();
    var idx = jsh.$root(jsh.App.xfilter_<%=model.id%>.PlaceholderID).children().index($(obj).parent());
    jsh.App.xfilter_<%=model.id%>.Data.Items.splice(idx,1);
    jsh.App.xfilter_<%=model.id%>.Render();
  }

  jsh.App.XForm<%=model.id%>_Export = function(){
    if(!jsh.App.xform_<%=model.id%>.Prop.isInlinePopup) if(jsh.XForm_OnGridExit(null,function(){ jsh.App.XForm<%=model.id%>_Export(); })) return;
    jsh.App.xform_<%=model.id%>.Load(0,0,function(url){
      jsh.getFileProxy().prop('src', url);
    },true);
  }

  jsh.App.XForm<%=model.id%>_UpdateTitleCaption = function(){
    var has_search_items = jsh.App.XForm<%=model.id%>_HasSearchItems(this.SearchJSON);
    var txt = '(';
    if(has_search_items) txt += 'filtered';
    if(jsh.App.xform_<%=model.id%>.DBRowCount >= 0) txt += ((txt=='(')?'':' - ') + jsh.App.xform_<%=model.id%>.DBRowCount.toLocaleString() + ' rows';
    if(txt == '(') txt = '';
    else txt += ')';
    if(txt){
      jsh.$root('.xtitlecaption<%=model.id%>').show();
    }
    else {
      jsh.$root('.xtitlecaption<%=model.id%>').hide();
    }
    jsh.$root('.xtitlecaption<%=model.id%> a').text(txt);
  }

})(<%-instance%>);

<%-jsh.RenderEJS('jsh_grid.js.datamodel',ejsparams)%>